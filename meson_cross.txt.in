[binaries]
c = '@CMAKE_INSTALL_PREFIX@/bin/@TARGET_ARCH@-clang'
cpp = '@CMAKE_INSTALL_PREFIX@/bin/@TARGET_ARCH@-clang++'
as = '@CMAKE_INSTALL_PREFIX@/bin/@TARGET_ARCH@-clang'
windres = '@CMAKE_INSTALL_PREFIX@/bin/@TARGET_ARCH@-windres'
dlltool = '@CMAKE_INSTALL_PREFIX@/bin/@TARGET_ARCH@-dlltool'
ar = '@CMAKE_INSTALL_PREFIX@/bin/@TARGET_ARCH@-ar'
ranlib = '@CMAKE_INSTALL_PREFIX@/bin/@TARGET_ARCH@-ranlib'
objcopy = '@CMAKE_INSTALL_PREFIX@/bin/@TARGET_ARCH@-objcopy'
strip = '@CMAKE_INSTALL_PREFIX@/bin/@TARGET_ARCH@-strip'
nasm = 'nasm'
pkg-config = 'pkg-config'
cmake = ''

[host_machine]
system = 'windows'
cpu_family = '@TARGET_CPU_FAMILY@'
cpu = '@TARGET_ARCH@'
endian = 'little'

[built-in options]
prefix = '@MINGW_INSTALL_PREFIX@'
libdir = '@MINGW_INSTALL_PREFIX@/lib'
wrap_mode= 'nofallback'
buildtype = 'release'
b_ndebug = 'true'
c_std = 'gnu17'
cpp_rtti = false
cpp_std = 'gnu++20'
default_library = 'static'
default_both_libraries = 'static'
prefer_static = true
pkgconfig.relocatable = true

[cmake]
CMAKE_TOOLCHAIN_FILE = '@TOOLCHAIN_FILE@'
